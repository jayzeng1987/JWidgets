<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JSimpleStepView">
        <attr name="isShowTitle" format="boolean" />
        <attr name="stepCircleRadius" format="dimension" />
        <attr name="stepNumTextSize" format="dimension" />
        <attr name="stepTitleTextSize" format="dimension" />
        <attr name="stepTitleTopSpace" format="dimension" />
        <attr name="stepLineStrokeWidth" format="dimension" />
        <attr name="baseStepLineStrokeWidth" format="dimension" />
        <attr name="undoStepColor" format="reference" />
        <attr name="currentStepColor" format="reference" />
        <attr name="finishStepColor" format="reference" />
        <attr name="finishStepLineColor" format="reference" />
        <attr name="baseStepLineColor" format="reference" />
        <attr name="nextStepLineColor" format="reference" />
        <attr name="stepNumTextColor" format="reference" />
        <attr name="stepStatusGraphType">
            <enum name="NUMBER" value="1" />
            <enum name="GRAPH" value="2" />
        </attr>

    </declare-styleable>
</resources>
